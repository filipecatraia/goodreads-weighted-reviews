{
  "manifest_version": 2,
  "name": "GoodReads Weighted Reviews",
  "description": "Reduces the value of 1 and 5 star reviews on goodreads.com.",
  "homepage_url": "https://github.com/mdn/webextensions-examples/tree/master/emoji-substitution",
  "version": "1.1",
  "icons": {
    "48": "icons/icon.png",
    "96": "icons/icon@2x.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.goodreads.com/*"],
      "js": ["./substitute.js"]
    }
  ]
}
